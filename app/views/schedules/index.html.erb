<div class="h-screen w-screen flex justify-center items-center">
  <%= month_calendar events: @schedules do |date, schedules| %>
    <%= date.day %>

    <% schedules.each do |schedule| %>
      <div>
        <% if schedule.got_up == true %>
          <p>しっかり起きれた！</p>
        <% end %>
      </div>
    <% end %>

  <% end %>



</div>